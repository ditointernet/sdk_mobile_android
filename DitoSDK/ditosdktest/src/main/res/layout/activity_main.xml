<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_identify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Realizar Identify" />

        <Button
            android:id="@+id/btn_register_token"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Register Token" />

        <Button
            android:id="@+id/btn_unregister_token"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:enabled="false"
            android:text="Unregister Token" />

        <Button
            android:id="@+id/btn_read_notification"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Read Notification" />

        <Button
            android:id="@+id/btn_alias"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Alias" />

        <Button
            android:id="@+id/btn_unalias"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Unalias" />

        <Button
            android:id="@+id/btn_track_event"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Track event" />

        <Button
            android:id="@+id/btn_track_event_offline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Track event Offline" />

        <Button
            android:id="@+id/btn_request_get"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Request GET" />

        <Button
            android:id="@+id/btn_request_post"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Request POST" />


    </LinearLayout>

</ScrollView>
